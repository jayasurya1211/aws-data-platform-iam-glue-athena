{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowListBucket",
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::company-data-lake-REPLACE_THIS"
        },
        {
            "Sid": "AllowReadCuratedObjects",
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::company-data-lake-REPLACE_THIS/curated/*"
        },
        {
            "Sid": "AllowAthenaQueries",
            "Effect": "Allow",
            "Action": [
                "athena:Get*",
                "athena:StartQueryExecution"
            ],
            "Resource": "*"
        }
    ]
}
